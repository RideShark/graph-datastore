  "bugs": {
    "url": "https://github.com/RideShark/graph-datastore/issues"
  },
  "homepage": "https://github.com/RideShark/graph-datastore#readme",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/lodash": "^4.14.54",
    "@types/mocha": "^2.2.38",
    "@types/sinon": "^1.16.35",
    "amdclean": "^2.7.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "requirejs": "^2.3.3",
    "sinon": "^1.17.7",
    "typescript": "^2.1.5"
  }
}
